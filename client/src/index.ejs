<!DOCTYPE html>
<html lang="en" ng-app="elementBoxApp">
<head>
  <title>Element Box</title>

  <link rel="icon" sizes="32x32" href="/favicon.png" type="image/png" />
  <link rel="shortcut icon" href="/favicon.ico">

  <!-- General META -->
  <meta charset="utf-8">
  <meta http-equiv="Content-type" content="text/html;charset=UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="fragment" content="!">

  <!-- Responsive META -->
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <meta name="apple-mobile-web-app-capable" content="yes">
  <meta name="apple-mobile-web-app-status-bar-style" content="black">

  <!-- Semantic META -->
  <meta name="author" content="/humans.txt">
  <meta name="description" content="Rent unique accommodations from local hosts in 190+ countries. Feel at home anywhere you go in the world with Element BoxLabs.">
  <meta name="keywords" content="">

  <link rel="stylesheet" type="text/css" href="css/bootstrap-styles.css"/>
  <link rel="stylesheet" type="text/css" href="css/styles.css"/>
</head>

<body ng-controller="AppController">
  <!-- Application content: -->
  <div class="container" ui-view></div>
  <!-- <div class="container view-animate" ui-view=""></div> -->

  <script type="text/javascript" src="js/angular.js"></script>
  <script type="text/javascript" src="js/ui-bootstrap-tpls.js"></script>

  <% modules.forEach(function(module) { %>
  <script type="text/javascript" src="js/modules/<%= module %>.js"></script>
  <% }); %>
  <% if (production) { %>
  <script type="text/javascript" src="js/modules.js"></script>
  <% } %>
  <script type="text/javascript" src="js/common.js"></script>
  <script type="text/javascript" src="js/<%= mainJsName %><%= production ? '.min' : '' %>.js"></script>

  <!-- Google analytics: -->
  <script>
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

    // ga('create', 'UA-66636677-1', 'auto');
    // ga('create', 'UA-66636677-1', {
    //   'cookieName': '_ga_',
    //   // 'cookieDomain': 'element-boxlabs.rhcloud.com',
    //   'cookieDomain': 'none',
    //   'cookieExpires': 60 * 60 * 24 * 28  // Time in seconds.
    // });
    ga('create', 'UA-66636677-1', 'auto');
    ga('send', 'pageview');
  </script>

</body>
</html>
